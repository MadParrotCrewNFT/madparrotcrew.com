{
  "name": "MadParrotCrew",
  "private": true,
  "description": "Website source code for madparrotcrew.com",
  "homepage": "https://madparrotcrew.com",
  "author": "Mad Parrot Crew team",
  "contributors": [
    "Sheftali (https://github.com/sheftali)",
    "Jack Domleo (https://jackdomleo.dev)",
    "FancyRatsDave (https://github.com/FancyRatsDave)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadParrotCrew/madparrotcrew.com.git"
  },
  "scripts": {
    "postinstall": "npm run typechain",
    "dev": "npm run typechain && nuxt --open",
    "start": "rimraf node_modules dist && npm ci && npm run generate && nuxt start --open",
    "generate": "concurrently \"rimraf dist\" \"npm run typechain\" && nuxt generate -m --fail-on-error",
    "typechain": "npx typechain --target=ethers-v5 --out-dir contract/types contract/out/MadParrotCrew.sol/MadParrotCrew.json"
  },
  "dependencies": {
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "core-js": "^3.23.4",
    "ethers": "^5.6.9",
    "focus-trap": "^6.9.4",
    "focus-trap-vue": "^1.1.1",
    "modern-normalize": "^1.1.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "^16.11.43",
    "babel-core": "7.0.0-bridge.0",
    "concurrently": "^7.2.2",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^10.3.1",
    "typechain": "^8.1.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
