{
  "name": "MadParrotCrew",
  "private": true,
  "description": "Website source code for madparrotcrew.com",
  "homepage": "https://madparrotcrew.com",
  "author": "Mad Parrot Crew team",
  "contributors": [
    "Sheftali (https://github.com/sheftali)",
    "Iacove (https://github.com/Iacove)",
    "Jack Domleo <jackdomleo7@gmail.com> (https://jackdomleo.dev)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadParrotCrew/madparrotcrew.com.git"
  },
  "scripts": {
    "postinstall": "npm run typechain",
    "dev": "npm run typechain && nuxt --open",
    "start": "concurrently \"rimraf node_modules\" \"rimraf dist\" && npm ci && npm run generate && nuxt start --open",
    "generate": "rimraf dist && nuxt generate -m --fail-on-error",
    "typechain": "npx typechain --target=ethers-v5 --out-dir contract/types contract/abi/MadParrotCrew.json"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "axios": "^0.25.0",
    "comma-number": "^2.1.0",
    "core-js": "^3.21.0",
    "ethers": "^5.5.4",
    "modern-normalize": "^1.1.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/node": "^17.0.14",
    "babel-core": "7.0.0-bridge.0",
    "concurrently": "^7.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^10.2.1",
    "typechain": "^7.0.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
