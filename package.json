    "nuxt": "^2.15.8",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@types/node": "^17.0.9",
    "babel-core": "7.0.0-bridge.0",
    "push-dir": "^0.4.1",
    "rimraf": "^3.0.2",
